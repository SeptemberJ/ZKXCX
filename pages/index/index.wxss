  animation:breath 1s;
  -webkit-animation:breath 1s infinite; /*Safari and Chrome*/
}
.ToggleArrow_Up image{
  width: 50rpx;
  height: 50rpx;
  transform: rotate(180deg);
  animation:breath 1s;
  -webkit-animation:breath 1s infinite; /*Safari and Chrome*/
}
@keyframes breath /*Safari and Chrome*/
{
    0% {
        opacity: 1;
    }
    50% {
        opacity: 0;
    }
    100% {
        opacity: 1;
    }
}
.TemperatureDetail{
