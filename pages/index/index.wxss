.CodeBlock{
  width: 500rpx;
  height: 500rpx;
  border-radius: 20rpx;
  /* border: 1px solid #efefef; */
  box-shadow: 0 4px 4px rgba(0,0,0,.2);
  position: absolute;
  top: 50%;
  left: 50%;
  z-index:999999;
  background: #fff;
  transform: translate(-50%,-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.CodeBlock image{
  width: 400rpx;
  height: 400rpx;
  display: block;
}
