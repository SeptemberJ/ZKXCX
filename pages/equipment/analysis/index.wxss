.leftBox{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.leftBox image{
  width: 80rpx;
  height: 80rpx;
}
.rightBox{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.rightBox{
  font-size: 14px;
}
