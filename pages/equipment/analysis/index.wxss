.equipmentInfo{
  width: calc(100% - 20px);
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 0 10px;
}

.equipmentCode{
  margin: 10px auto;
  font-size: 14px;
}
.DataList{
  width: 750rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-around;
  margin: 30px auto;
}
.DataItem{
  width: 340rpx;
  height: 100px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  box-shadow: 0rpx 0rpx 5px rgba(0,0,0,.3);
  margin-bottom: 20px;
}
